<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!--实现应用背景颜色渐变-->
    <gradient
        android:startColor="@color/gray"
        android:endColor="@color/gray"
        android:angle="0"/>

    <corners android:radius="1dp" />
    <padding
        android:left="1dp"
        android:top="1dp"
        android:right="1dp"
        android:bottom="1dp" />
    <stroke
        android:color="@color/black"
        android:width="1dp"/>
</shape>
